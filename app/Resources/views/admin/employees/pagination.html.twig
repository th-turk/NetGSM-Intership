<div class="bd-example text-center" >
    <nav aria-label="Page navigation" >

        <ul class="pagination justify-content-center">

            {% for page in 1..pages %}

                {% if app.request.get("numb") ==  page  %}
                <li >
                    <a class=" active" href="{{ path('all_employees',{"numb":page}) }}">
                        {{ page }}
                    </a>
                </li>
                {% else %}
                    <li >
                        <a class="" href="{{ path('all_employees',{"numb":page}) }}">
                            {{ page }}
                        </a>
                    </li>
                {% endif %}
            {% endfor %}

        </ul>
    </nav>
</div>